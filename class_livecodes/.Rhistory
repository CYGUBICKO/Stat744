#### Setup
rm(list = ls())
#### Packages
library(ggplot2)
library(directlabels)
theme_set(theme_bw())
library(dplyr)
# ---- Data ----
## Read data
banana_df <- read.csv("https://raw.githubusercontent.com/thomasdebeus/colourful-facts/develop/projects/Redesign-of-worst-chart-ever/FAOSTAT_data_1-7-2018.csv")
# ---- Vizualization ----
banana_plot <- (ggplot(banana_df, aes(x = Year, y = Value/1000, colour = Country))
+ geom_line()
+ labs(y = "Bananas ('000)")
)
direct.label(banana_plot +
scale_x_continuous(lim = c(1995, 2007), breaks = 1995:2005)
)
(banana_plot
+ scale_color_brewer(palette = "Paired")
+ scale_x_continuous(lim = c(1995, 2007), breaks = 1995:2005)
+ theme(legend.position = "none")
+ geom_dl(aes(label = Country), method = "last.bump", size = 1)
)
# ---- Class example: Banana example ----
